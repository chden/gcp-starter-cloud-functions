{
  "name": "gcp-starter-cloud-function",
  "version": "1.0.0",
  "description": "Simple starter project for Google Cloud Function.",
  "main": "scripts/index.js",
  "entrypoint": "main",
  "runtime": "nodejs12",
  "memory": "128MB",
  "scripts": {
    "test": "mocha --recursive ./test/**/*.unit.test.js",
    "test:debug": "npm run test -- --nolazy --inspect-brk=9229",
    "test:system": "mocha --recursive ./test/**/*.system.test.js",
    "lint": "npx eslint scripts test",
    "start": "functions-framework --source=scripts --target=${npm_package_entrypoint}",
    "start:debug": "node --inspect node_modules/@google-cloud/functions-framework --source=scripts --target=${npm_package_entrypoint}",
    "deploy": "gcloud functions deploy ${npm_package_name} --trigger-http --runtime ${npm_package_runtime} --memory ${npm_package_memory} --entry-point ${npm_package_entrypoint}"
  },
  "author": "chden",
  "contributors": [],
  "license": "MIT",
  "dependencies": {
    "@google-cloud/functions-framework": "^1.6.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.4.0",
    "eslint-config-google": "^0.14.0",
    "generate-package-json-webpack-plugin": "^1.1.2",
    "mocha": "^8.0.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "sinon-express-mock": "^2.2.1"
  }
}
